<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zzh.vae">

    <!--读写SD卡-->
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.SurfaceActivity" />
        <activity
            android:name=".activity.SlidingActivity"
            android:theme="@style/AppTheme" />
        <activity android:name="com.zzh.html5.activity.Html5Activity" />
        <activity android:name="com.zzh.player.PlayerActivity" />
        <activity android:name="com.zzh.facedetector.FacedetectorActivity"></activity>
    </application>

</manifest>
