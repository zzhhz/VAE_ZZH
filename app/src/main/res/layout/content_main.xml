<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.zzh.vae.MainActivity"
    tools:showIn="@layout/activity_main">
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="跳转到侧滑菜单"
        android:id="@+id/button"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="测试WebView"
        android:id="@+id/button2"
        android:layout_below="@+id/button"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="播放视频"
        android:id="@+id/button3"
        android:layout_below="@+id/button2"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="选择图片"
        android:id="@+id/button4"
        android:layout_below="@+id/button3"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="生成二维码"
        android:id="@+id/button5"
        android:layout_below="@+id/button4"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="测试PullToZoomRecyclerView"
        android:id="@+id/button6"
        android:layout_below="@+id/button5"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="测试PullToZoomScrollView"
        android:id="@+id/button7"
        android:layout_below="@+id/button6"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="测试选择图片"
        android:id="@+id/button8"
        android:layout_below="@+id/button7"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="权限测试"
        android:id="@+id/button9"
        android:layout_below="@+id/button8"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="扫描二维码"
        android:id="@+id/button10"
        android:layout_below="@+id/button9"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="网络访问"
        android:id="@+id/button11"
        android:layout_below="@+id/button10" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="测试瀑布流"
        android:id="@+id/button12"
        android:layout_below="@+id/button11" />
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="播放网络视频"
        android:id="@+id/button13"
        android:layout_below="@+id/button12" />
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="登录"
        android:id="@+id/button14"
        android:layout_below="@+id/button13" />
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Retrofit"
        android:id="@+id/button15"
        android:layout_below="@+id/button14" />
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button15"
        android:id="@+id/button16"
        android:text="测试高斯模糊"/>
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button16"
        android:id="@+id/button17"
        android:text="测试高斯模糊"/>
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button17"
        android:id="@+id/button18"
        android:text="弹出框"/>
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button18"
        android:id="@+id/button19"
        android:text="Ok-http"/>
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button19"
        android:id="@+id/button20"
        android:text="圆形图片"/>
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button20"
        android:id="@+id/button21"
        android:text="动画"/>
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button21"
        android:id="@+id/button22"
        android:text="recyclerview侧滑菜单"/>
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button22"
        android:id="@+id/button23"
        android:text="测试侧滑菜单"/>
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button23"
        android:id="@+id/button24"
        android:text="Surface Activity"/>
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button24"
        android:id="@+id/button25"
        android:text="Folder cell"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="网络视频播放"
        android:id="@+id/button26"
        android:layout_alignParentTop="true"
        android:layout_toEndOf="@+id/button2"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="属性动画"
        android:id="@+id/button27"
        android:layout_above="@+id/button2"
        android:layout_toEndOf="@+id/button26"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="属性实现基本的动画"
        android:id="@+id/button28"
        android:layout_alignTop="@+id/button2"
        android:layout_toEndOf="@+id/button2"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="布局动画"
        android:id="@+id/button29"
        android:layout_alignTop="@+id/button28"
        android:layout_toEndOf="@+id/button28"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Folder cell 自定义"
        android:id="@+id/button30"
        android:layout_alignTop="@+id/button3"
        android:layout_toEndOf="@+id/button3"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="名医专家"
        android:id="@+id/button31"
        android:layout_alignTop="@+id/button30"
        android:layout_toEndOf="@+id/button30"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="转场动画"
        android:id="@+id/button32"
        android:layout_below="@+id/button3"
        android:layout_toEndOf="@+id/button3"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="测试对话框"
        android:id="@+id/button33"
        android:layout_below="@+id/button30"
        android:layout_toRightOf="@+id/button32"
        android:layout_toEndOf="@+id/button32"/>
</RelativeLayout>
